{
  "name": "Crevice",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "VI key bindings",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "history",
    "tabs",
    "sessions",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": ["src/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [ "src/inject/crevice.css" ],
      "js": [
        "src/inject/util.js",
        "src/inject/input_nodes.js",
        "src/inject/actions.js",
        "src/inject/scroll.js",
        "src/inject/key.js",
        "src/inject/keyhandler.js",
        "src/inject/hints.js",
        "src/inject/commander.js",
        "src/inject/command_input.js",
        "src/inject/command_line.js",
        "src/inject/searchnode.js",
        "src/inject/search.js",
        "src/inject/tabs.js",
        "src/inject/marks.js",
        "src/inject/operator.js",
        "src/inject/map.js",
        "src/inject/main.js"
      ]
    }
  ]
}
