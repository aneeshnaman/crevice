{
  "name": "Crevice",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "VI key bindings",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "history",
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "src/inject/util.js",
        "src/inject/key.js",
        "src/inject/keyhandler.js",
        "src/inject/searchnode.js",
        "src/inject/search.js",
        "src/inject/actions.js",
        "src/inject/map.js",
        "src/inject/main.js"
      ]
    }
  ]
}
